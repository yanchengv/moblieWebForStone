<% if !@sticky_pages.nil?%>

    <%for sticky_page in @sticky_pages%>
        <%if !sticky_page.content_for(:home_part1).nil?%>
            <%= raw sticky_page.content_for(:home_part1)%>
        <%end%>
    <%end%>

    <div class="row margin-bottom-20">
      <%for sticky_page in @sticky_pages%>
          <%if !sticky_page.content_for(:home_part2).nil?%>
              <%= raw sticky_page.content_for(:home_part2)%>
          <%end%>
      <%end%>
    </div>

<%end%>

<!--scroll jquery-->
<script>
    var slider = Swipe(document.getElementById('scroll_img'), {
        auto: 3000,
        continuous: true,
        callback: function(pos) {
            var i = bullets.length;
            while (i--) {
                bullets[i].className = ' ';
            }
            bullets[pos].className = 'on';
        }
    });
    var bullets = document.getElementById('scroll_position').getElementsByTagName('li');
    $(function(){
        $('.scroll_position_bg').css({
            width:$('#scroll_position').width()
        });
    });
</script>